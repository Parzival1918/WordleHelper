    "#Read words in file web2 and store the ones that are 5 letters long in a file called words5\n",
    "wordLength = 5\n",
    "with open('web2') as f:\n",
    "    with open('words' + str(wordLength), 'w') as f2:\n",
    "        for line in f:\n",
    "            if len(line.strip()) == wordLength:\n",
    "                f2.write(line)\n"
   ]
